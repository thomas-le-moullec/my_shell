test_12()
{
  kill_shell 2>&1 > /dev/null
  touch "lagoudale.$$"
  launch_shell 2>&1 > /dev/null
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "mkdir tmp.$$" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "cd tmp.$$" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "pwd" > $PIPE
  fi
  sleep 0.3
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "cd -" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "rm -Rf tmp.$$" > $PIPE
  fi
  sleep 0.3
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "ls" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "cd /" > $PIPE
  fi
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "ls" > $PIPE
  fi
  sleep 0.5
  a=`cat $OUT | grep "tmp.$$" | wc -l`
  b=`cat $OUT | grep "lagoudale.$$" | wc -l`
  c=`cat $OUT | grep "usr" | wc -l`
  if [ $a -eq 1 -a $b -eq 1 -a $c -eq 1 ]
  then
    echo "OK "
  else
    echo "KO "
  fi

  rm -f lagoudale.$$ 2>&1 > /dev/null
}

