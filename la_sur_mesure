  then
      echo "exit 42" > $PIPE
  fi
  sleep 0.5
  a=`ps aux | grep -v grep | grep $USER | grep $SHELL | wc -l`
  if [ $a -eq 0 ]
  then
    echo "OK "
  else
    echo "KO "
  fi
  rm -f tmp.$$ 2>&1 > /dev/null
}

