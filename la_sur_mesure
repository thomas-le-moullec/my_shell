test_05()
{
  kill_shell 2>&1 > /dev/null
  launch_shell 2>&1 > /dev/null
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
    echo "mkdir tmp" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
    echo "cd tmp" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
    echo "touch lagoudale.$$" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
    echo "/bin/ls -la" > $PIPE
  fi
  sleep 0.5
  a=`cat $OUT | grep "total 8" | wc -l`
  b=`cat $OUT | grep "lagoudale.$$" | wc -l`

  if [ $a -eq 1 -a $b -eq 1 ]
  then
    echo "OK"
  else
    echo "KO"
  fi

  rm -Rf tmp 2>&1 > /dev/null
}

