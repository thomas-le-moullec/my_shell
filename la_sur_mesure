      echo "cd tmp" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "touch lagoudale.$$" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "      ls -l" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "      ls      -l" > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "      ls      -l      " > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "      ls      -l     -a     " > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "      ls      -l     -a   -l  " > $PIPE
  fi
  sleep 0.2
  if [ `pidof $SHELL | wc -l` -eq 1 ]
  then
      echo "      ls      -l     -a   -      l  " > $PIPE
  fi
  sleep 0.5
  a=`cat $OUT | grep "lagoudale.$$" | wc -l`
  b=`cat $OUT | grep "total 8" | wc -l`
  if [ $a -eq 5 -a $b -eq 2 ]
  then
    echo "OK "
